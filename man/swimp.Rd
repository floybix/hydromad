\name{swimp}
\Rdversion{1.1}
\alias{swimp}
\title{
Simple Wetland Inundation Model using Poweroids
}
\description{
Model flood area / duration / depth in wetlands.
}
\usage{
swimp(flow.ML, thresh = 0,
      alpha, beta,
      E.mm = 0, P.mm = 0,
      Ksat.mm.day = 0,
      e = 0.2, g = 140,
      Hmax = 2000, Amax = 10000,
      porosity = 0.2,
      M_0 = Hmax * porosity, V_0 = 0,
      drainage = 0, drainLevel = 0)
}
\arguments{
  \item{flow.ML}{
    inflow or streamflow in ML per timestep.
}
  \item{thresh}{
    a threshold for \code{flow.ML}, such that only flow above this value
    enters the wetland.
}
  \item{alpha, beta}{
    parameters defining the shape of the wetland. See \code{\link{poweroid}}.
}
  \item{E.mm, P.mm}{
    potential evapo-transpiration and precipitation in mm per timestep.
  }
  \item{Ksat.mm.day}{
    Saturated hydraulic conductivity in mm per timestep, relative to a
    reference pressure of 10cm. If this is 0, the wetland surface water
    is isolated from the surrounding water table, i.e. there is no
    infiltration nor discharge.
}
  \item{e}{
    %%     ~~Describe \code{e} here~~
}
  \item{g}{
    stress threshold in terms of Catchment Moisture Deficit (mm), as in
    the IHACRES CMD model, where \code{g = f * d}. See
    \code{\link{IHACRES.CMD.model}}.
}
  \item{Hmax}{
%%     ~~Describe \code{Hmax} here~~
}
  \item{Amax}{
%%     ~~Describe \code{Amax} here~~
}
  \item{porosity}{
    effective porosity of the soil.
}
  \item{M_0}{
    initial value of Catchment Moisture Deficit, mm.
}
  \item{V_0}{
    initial volume of surface water in wetland, ML.
  }
  \item{drainage}{
    drainage rate as a proportion of volume above \code{drainLevel} per timestep.
  }
  \item{drainLevel}{
    water level (millimetres from base) above which drainage occurs.
  }
}
\details{
  ...
}
\value{
  a \code{\link{zoo}} object (time series object).
}
\references{
  ...
}
\author{
  Felix Andrews \email{felix@nfrac.org}
}
\seealso{
  \code{\link{poweroid}},
  \code{\link{convertFlow}}
}
\examples{
## assume Q is inflow in ML/day
set.seed(1)
Q <- rpois(100, lambda = 0.1) * 1000
## assume depth distribution follows a cone, i.e. beta = 1
## estimate alpha given known area vs volume
## lets say a volume of 1000 ML corresponds to area 20 km^2
alpha <- poweroid(V = 1000, A = 20, beta = 1)$alpha
flood <- swimp(Q, alpha = alpha, beta = 1, E.mm = 10)
head(flood, 20)
xyplot(flood)
}
\keyword{ math }
\keyword{ models }

